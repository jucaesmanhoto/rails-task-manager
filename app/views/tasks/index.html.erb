<!-- <div class="container"> -->
  <h1>My Tasks</h1>

  <div class="form-check mt-4">
    <% @tasks.each do |task| %>
      <div class="row col-12">
        <input class="form-check-input" type="checkbox" value="">
        <label class="form-check-label" for="defaultCheck1">
          <%= link_to task.title, task_path(task) %>
        </label>
      </div>
    <% end %>
  </div>

